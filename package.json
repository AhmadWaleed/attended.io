{
  "private": true,
  "scripts": {
    "dev": "$npm_execpath run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "$npm_execpath run development -- --watch",
    "watch-poll": "$npm_execpath run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "$npm_execpath run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "format": "prettier --write '**/*.{css,js,json}'",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^11.11.3",
    "@types/react": "^16.8.8",
    "cross-env": "^5.1",
    "laravel-javascript-views": "^0.2.1",
    "laravel-mix": "^4.0.7",
    "laravel-mix-purgecss": "^4.1.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "prettier": "^1.16.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "resolve-url-loader": "2.3.1",
    "tailwindcss": "^0.7.4",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3333",
    "vue-template-compiler": "^2.6.6"
  },
  "dependencies": {
    "@types/webpack-env": "^1.13.9"
  }
}
